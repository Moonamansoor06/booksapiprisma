datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
model books {
  id       Int     @id @default(autoincrement())
  bookname String
  author   String
  booktype String
  price   Int
  qty      Int
  isbn     String
}
generator client {
  provider = "prisma-client-js"
}