datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}
model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
model books {
  id       Int     @id @default(autoincrement())
  bookname String
  booktype String
  author   String
   qty      Int 
  isbn     String
  price   Int
 
  
}
generator client {
  provider = "prisma-client-js"
}